    /**
     * 
     * @param param_semester  => "pilihan mengambil data nilai dari semester berapa"
     * loadData untuk mengambil data dari db
     */
    private void loadData(String param_semester) {                        
        DefaultTableModel dtm = new DefaultTableModel(){
            @Override
            public boolean isCellEditable(int i, int i1) {
                return false;
            }            
        };
        
        SQLStatment sqls = new SQLStatment();                                
        try {
            ResultSet data = sqls.selectQuery(null, null, "SELECT nama_pelajaran,"+param_semester+" FROM tb_nilai WHERE nis ="+txtNIS.getText().toString());
            dtm.setColumnCount(2);
            
            Object[] header = {"Nama Pelajaran", "Nilai"};                
            dtm.setColumnIdentifiers(header);
            
            while(data.next()){
                
                if(Float.parseFloat(data.getString(param_semester)) != 0){
                    Object[] os = {data.getString("nama_pelajaran"), data.getString(param_semester)};                
                    dtm.addRow(os);
                    jTable1.setModel(dtm);
                }
            }
            
        } catch (SQLException ex) {
            Logger.getLogger(DetailFrame.class.getName()).log(Level.SEVERE, null, ex);
        }
        
    }

