    }
    
    /**
     * 
     * @param isSnmptn => report untuk snmptn ? true : false;
     * @param option  => print, view atau export
     * 
     * actionForReport Berfungsi untuk setiap action pada report
     */
    private void actionForReport(boolean isSnmptn, int option) {
                
            try {

                classLoader = getClass().getClassLoader();
                inputStream  = classLoader.getResourceAsStream(!isSnmptn ? getReportName(status_print) : "SNMPTN.jasper");
                param.put("nis", Integer.parseInt(txtNIS.getText().toString()));
                
                try {
                    jasperPrint = JasperFillManager.fillReport(inputStream, param, MyConnection.getConnection());
                } catch (JRException ex) {
                    Logger.getLogger(DetailFrame.class.getName()).log(Level.SEVERE, null, ex);
                }
                
                
                switch(option){
                    case VIEW_REPORT:
                        JasperViewer.viewReport(jasperPrint, false);
                        break;
                    case EXPORT_PDF:  
                        String path = getPath();
                        if(path != null && !path.equalsIgnoreCase("")){
                            JasperExportManager.exportReportToPdfFile(jasperPrint, path);                            
                        }
                        break;
                    case PRINT_REPORT:
                        JasperPrintManager.printPage(jasperPrint, 0 , true);
                        break;
                }
            } catch (JRException ex) {
                Logger.getLogger(DetailFrame.class.getName()).log(Level.SEVERE, null, ex);
            }
    }
    
    /**
     * setData untuk setter data
     * buat kaya intent di android :D
     * dan langsung loadData dari db
     * 
     * @param obj 
     */
    public void setData(Object[] obj){        
        txtNIS.setText(String.valueOf(obj[0]));
        txtNama.setText(String.valueOf(obj[1]));
        txtJurusan.setText(String.valueOf(obj[2]));
        txtKelas.setText(String.valueOf(obj[3]));
        txtTTL.setText(String.valueOf(obj[4]));
        
        loadData(NILAI_SEMESTER1);
    }
    
    
    public String getReportName(String statusPrint){
        if(statusPrint.equalsIgnoreCase(NILAI_SEMESTER1)){
            return "RapotSiswa.jasper";
        }else if(statusPrint.equalsIgnoreCase(NILAI_SEMESTER2)){
            return "RapotSiswaSMT2.jasper";
        }else if(statusPrint.equalsIgnoreCase(NILAI_SEMESTER3)){
            return "RapotSiswaSMT3.jasper";
        }else if(statusPrint.equalsIgnoreCase(NILAI_SEMESTER4)){
            return "RapotSiswaSMT4.jasper";
        }else if(statusPrint.equalsIgnoreCase(NILAI_SEMESTER5)){
            return "RapotSMT5.jasper";
        }else if(statusPrint.equalsIgnoreCase(NILAI_SEMESTER6)){
            return "RapotSiswaSMT6.jasper";
        }else{
            return "RapotSiswa.jasper";
        }
    }
    
    public String getPath(){
        String path ="";            
            JFileChooser chooser = new JFileChooser();
            chooser.addChoosableFileFilter(new FileNameExtensionFilter(".pdf", "pdf"));
            chooser.setDialogTitle("Pilih Lokasi Penyimpanan");
            File fileToSave = null;
            
            int UserSelect = chooser.showSaveDialog(this);
            
            if(UserSelect== JFileChooser.APPROVE_OPTION){                
                fileToSave = chooser.getSelectedFile();                
                path = fileToSave.getAbsolutePath();   
                
                if(fileToSave.getName() == null){
                    fileToSave = new File(path + "untitled.pdf");
                }                
                
                if(!path.contains(".pdf")){
                    fileToSave = new File(path + ".pdf");                    
                }                                
            }                                                            
                                
            return fileToSave.getAbsoluteFile().toString() != null ? fileToSave.getAbsoluteFile().toString() : "";
    }

